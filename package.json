{
  "name": "testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all start-dev start-mockapi",
    "start-server": "npm-run-all start-dev start-mockapi",
    "start-dev": "react-scripts -r @cypress/instrument-cra start",
    "prestart-mockapi": "node buildScripts/generateMockData",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001 --static node_modules/json-server/lib/server/public",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "coverage": "react-scripts test --env=jsdom --coverage ",
    "open:coverage": "npm run coverage && opn ./coverage/lcov-report/index.html",
    "cypress": "cypress open",
    "eject": "react-scripts eject",
    "dev": "start-test 3000 cypress",
    "test:jest": "jest",
    "test:cy": "cypress run",
    "test": "npm run test:cy",
    "pretest": "rm -rf .nyc_output || true",
    "posttest": "npm run report:combined",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "npm run mkdir:reports",
    "copy:reports": "cp cypress-coverage/coverage-final.json reports/from-cypress.json && cp jest-coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "npm run copy:reports && mkdir .nyc_output || true",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "npm run combine:reports",
    "report:combined": "npx nyc report --reporter lcov --reporter text --report-dir coverage",
    "ci": "start-server-and-test start-server http://localhost:3000 test"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "2.1.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@cypress/code-coverage": "1.8.0",
    "@cypress/instrument-cra": "1.3.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "cypress": "^3.8.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "istanbul-lib-coverage": "2.0.5",
    "jest": "^23.6.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.16.3",
    "mockdate": "^2.0.5",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "nyc": "14.1.1",
    "opn-cli": "^4.1.0",
    "react-test-renderer": "^16.14.0",
    "react-testing-library": "^5.9.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "1.9.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nyc": {
    "report-dir": "cypress-coverage"
  }
}
